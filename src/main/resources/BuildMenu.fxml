<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.canvas.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<ScrollPane fitToWidth="true" hbarPolicy="NEVER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <content>
      <VBox alignment="CENTER">
         <children>
            <Button fx:id="runMode" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Running mode">
               <padding>
                  <Insets bottom="10.0" top="10.0" />
               </padding>
               <VBox.margin>
                  <Insets bottom="15.0" left="10.0" right="10.0" top="15.0" />
               </VBox.margin>
            </Button>
            <TitledPane expanded="false" text="File">
               <content>
                  <VBox>
                     <children>
                        <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onOpenButtonClicked" text="Open">
                           <VBox.margin>
                              <Insets bottom="20.0" />
                           </VBox.margin>
                        </Button>
                        <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onSaveButtonClicked" text="Save">
                           <VBox.margin>
                              <Insets bottom="20.0" />
                           </VBox.margin>
                        </Button>
                        <Button layoutX="24.0" layoutY="83.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onCloseButtonClicked" text="Close">
                           <VBox.margin>
                              <Insets bottom="20.0" />
                           </VBox.margin>
                        </Button>
                        <Button layoutX="24.0" layoutY="142.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onQuitButtonClicked" text="Quit">
                           <VBox.margin>
                              <Insets bottom="20.0" />
                           </VBox.margin>
                        </Button>
                     </children>
                  </VBox>
               </content>
            </TitledPane>
            <TitledPane expanded="false" text="Editing tools">
               <content>
                  <VBox>
                     <children>
                        <MenuButton maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Add New Gizmo">
                           <items>
                              <MenuItem mnemonicParsing="false" text="Absorber" />
                              <MenuItem mnemonicParsing="false" text="Ball" />
                              <MenuItem mnemonicParsing="false" text="Circle" />
                              <MenuItem mnemonicParsing="false" text="Left Flipper" />
                              <MenuItem mnemonicParsing="false" text="Right Flipper" />
                              <MenuItem mnemonicParsing="false" text="Square" />
                              <MenuItem mnemonicParsing="false" text="Triangle" />
                           </items>
                           <VBox.margin>
                              <Insets bottom="20.0" />
                           </VBox.margin>
                        </MenuButton>
                        <Separator prefWidth="200.0">
                           <VBox.margin>
                              <Insets bottom="20.0" />
                           </VBox.margin>
                        </Separator>
                        <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Move">
                           <VBox.margin>
                              <Insets bottom="20.0" />
                           </VBox.margin>
                        </Button>
                        <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Rotate">
                           <VBox.margin>
                              <Insets bottom="20.0" />
                           </VBox.margin>
                        </Button>
                        <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Connect">
                           <VBox.margin>
                              <Insets bottom="20.0" />
                           </VBox.margin>
                        </Button>
                        <Button layoutX="24.0" layoutY="224.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Disconnect">
                           <VBox.margin>
                              <Insets bottom="20.0" />
                           </VBox.margin>
                        </Button>
                        <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Delete">
                           <VBox.margin>
                              <Insets bottom="20.0" />
                           </VBox.margin>
                        </Button>
                        <Button layoutX="24.0" layoutY="283.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Clear board">
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                        </Button>
                     </children>
                  </VBox>
               </content>
            </TitledPane>
            <TitledPane expanded="false" text="Settings">
               <content>
                  <VBox>
                     <children>
                        <Label alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Gravity: 10 L" textAlignment="CENTER" />
                        <Slider fx:id="gravity" majorTickUnit="20.0" minorTickCount="4" onMouseReleased="#changeGravity" value="10.0">
                           <VBox.margin>
                              <Insets bottom="20.0" />
                           </VBox.margin>
                        </Slider>
                        <Separator prefWidth="200.0">
                           <VBox.margin>
                              <Insets bottom="20.0" />
                           </VBox.margin>
                        </Separator>
                        <Label alignment="CENTER" layoutX="24.0" layoutY="24.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Friction (mu): 0.025" textAlignment="CENTER" />
                        <Slider fx:id="friction1" blockIncrement="0.1" layoutX="24.0" layoutY="51.0" majorTickUnit="0.2" max="1.0" minorTickCount="4" onMouseReleased="#changeFricitonCoefficient1" value="0.025" />
                        <Label alignment="CENTER" layoutX="24.0" layoutY="115.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Friction (mu2): 0.025" textAlignment="CENTER" />
                        <Slider fx:id="friction2" blockIncrement="0.1" layoutX="24.0" layoutY="142.0" majorTickUnit="0.2" max="1.0" minorTickCount="4" onMouseReleased="#changeFricitonCoefficient2" value="0.025" />
                     </children>
                  </VBox>
               </content>
            </TitledPane>
         </children>
      </VBox>
   </content>
</ScrollPane>
